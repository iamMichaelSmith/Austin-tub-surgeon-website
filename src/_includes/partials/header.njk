<header class="header">
  <div class="container nav">
    <a class="logo" href="/">Austin <span>Tub Surgeon</span></a>
    <nav class="nav-links" data-nav-links>
      {% for item in site.nav %}
      <a href="{{ item.url }}" class="{% if pageKey == item.key %}active{% endif %}">{{ item.label }}</a>
      {% endfor %}
    </nav>
    <div class="nav-cta">
      <a class="button secondary" href="{{ site.phoneHref }}">Call {{ site.phone }}</a>
      <a class="button" href="/fast-quote/">Upload Photos for Fast Quote</a>
    </div>
    <button class="menu-toggle" data-menu-toggle>Menu</button>
  </div>
</header>
